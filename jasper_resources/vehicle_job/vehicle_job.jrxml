	<parameter name="vehicle_job_number" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
  vj.planned_location,
  s.location_code as location_code,
  vju.unit_reference_id,
  vj.vehicle_job_number,
  vj.id as vehicle_job_id,
  f.farm_code,
  r.commodity_code,
  r.variety_code,v.variety_description,
  r.product_class_code,
  r.size_code,
  p.pack_material_product_code,
  ti.track_slms_indicator_code,
  cast(b.weight as decimal(7,2)) as weight,
  now() as datenow,
  b.coldstore_type,
  r.treatment_code as colour,pc_code_code,date(now()) - date(b.created_on) AS fruit_age
FROM
  public.vehicle_job_units vju
  INNER JOIN public.vehicle_jobs vj ON (vju.vehicle_job_id = vj.id)
  INNER JOIN public.bins b ON (vju.unit_reference_id = b.bin_number)
  INNER JOIN public.farms f ON (b.farm_id = f.id)
  INNER JOIN public.rmt_products r ON (b.rmt_product_id = r.id)
  INNER JOIN public.ripe_points q ON (r.ripe_point_id = q.id)
  left JOIN public.varieties v on (v.id = r.variety_id)
  INNER JOIN public.pack_material_products p ON (b.pack_material_product_id = p.id)
  inner join public.stock_items s on (b.bin_number = s.inventory_reference)
  left join public.deliveries d on (b.delivery_id = d.id)
  left join public.track_slms_indicators ti on ti.id = b.track_indicator1_id
where vj.vehicle_job_number=$P{vehicle_job_number}]]>
	</queryString>
