
<html>
  <head>
  
    <style> body, html {margin:0px; padding: 0px;} </style>
    <link href= "/stylesheets/scaffold.css" media="screen" rel="Stylesheet" type="text/css" />
    <link href="/stylesheets/jskr1/jquery-ui.css" rel="stylesheet" type="text/css"/>
    <script src = "/javascripts/popupwindow.js" type="text/javascript"></script> 
    <script src = "/javascripts/lib/prototype.js" type="text/javascript"></script>
    <script src = "/javascripts/src/effects.js" type="text/javascript"></script>

    <script src = "/javascripts/jquery.min.js" type="text/javascript"></script>

    <script src = "/javascripts/jquery-ui.min.js" type="text/javascript"></script>
    <script src = "/javascripts/ui.multiselect.js" type="text/javascript"></script>
    <script src = "/javascripts/grid.locale-en.js" type="text/javascript"></script>
    <script src = "/javascripts/jqgrid_no_legacy.js" type="text/javascript"></script>
    <script src = "/javascripts/jquery.jqGrid.min.js" type="text/javascript"></script>
    <script src = "/javascripts/jqgrid_utils.js" type="text/javascript"></script>
    <script src = "/javascripts/jquery.collapse.js" type="text/javascript"></script>
    <script src = "/javascripts/jquery-ui-timepicker-addon.js" type="text/javascript"></script>
    <script src = "/javascripts/search_form.js" type="text/javascript"></script>
    <script src = "/javascripts/util.js" type="text/javascript"></script>
    <script src = "/javascripts/chosen.jquery.min.js" type="text/javascript"></script>
    
    <script language="JavaScript">
      // Block use of $ for jQuery to avoid conflict with Prototype.
      jQuery.noConflict();

      jQuery(document).ready(function() {
        jQuery(".chosen-select").chosen({disable_search_threshold: 10, allow_single_deselect: true, search_contains: true});
      });

        	function call_open_window(id)
	{
	open_window_link(id.id);
	}
        <% if session[:alert] != nil %>
            alert("<%= session[:alert] %>");
            <% session[:alert]= nil %>
        <%end%>
        
        <% @hide_content_pane  %>
    	function show_element(id)
  		{
    		elt = document.getElementById(id);
    		elt.style.display = "inline";
  
  		}
  	
  	<%= @tree_actions %>
  	
  	function content_load_complete()
  	{
  	   <% if @is_menu_loaded_view %>
         if (window.parent.stop_spinner) {
  	     window.parent.stop_spinner();
         }
  	   <%end %>
  	   
  	   <% if not @freeze_flash%>
         if ($('notice') != undefined) {
  	     Effect.SwitchOff('notice');
         Effect.Fade('notice');
       }
     <% end %>	
  	}
  	
  	
  	  //var function_map = new ArrayList();
  	
  	</script>
  	
  	<%= @header_content %>
  	
    <title>Admin</title>
    <%#= stylesheet_link_tag "scaffold", "depot", "admin" %>
    <%= stylesheet_link_tag "depot", "admin" %>
    <%= stylesheet_link_tag "chosen.css" %>
   
	
  </head>
  <body  onload ="content_load_complete();">
    <!--<div id="banner">
      <%= @page_title  %>
    </div>  -->    
     
    <div id="tree_node_content" style = "vertical-align: top;" > 
        
        <% if flash[:notice] -%>
          <div id="notice"><%= flash[:notice] %></div>
        <% end -%>
        <% if flash[:error] -%>
          <div id="error" class = "error"><%= flash[:error] %></div>
          <%flash[:error] = nil%>
        <% end -%>
        
     <% if not @hide_content_pane %>
	       <table width = '100%' height = '100%' cellspacing = "0" >
	       <tr height = '1%'>
	           <td id = "tree_node_content_header" width = '100%' style="font-size: smaller; border-right: gray 1px solid; border-top: gray 1px solid; background-image: url(/images/tlo_podmenu.gif); border-left: gray 1px solid; border-bottom: none">
		          <%= @tree_node_content_header %>
		       </td>
		   </tr>
		   <tr height = '100%'>
		     <td valign = "top" align ="left" width = '100%'style="padding-left: 20px; padding-top: 20px; border-right: gray 1px solid; border-left: gray 1px solid; border-bottom: gray 1px solid; border-top: none">
		          <%= @content_for_layout %>
		     </td>
		   </tr>
		</table>
		<%end%>
   </div> 
  </body>
</html>
